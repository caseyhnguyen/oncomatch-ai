# OncoMatch AI Environment Configuration
# Copy this file to .env and add your API keys

# ===========================
# REQUIRED: LLM API Keys (need at least one)
# ===========================

# OpenAI API Key - for GPT models
OPENAI_API_KEY=your_openai_key_here

# Anthropic API Key - for Claude models  
ANTHROPIC_API_KEY=your_anthropic_key_here

# Google API Key - for Gemini models (RECOMMENDED for cost-effectiveness)
GOOGLE_API_KEY=your_google_key_here

# ===========================
# OPTIONAL BUT RECOMMENDED: Enhanced Data Sources
# ===========================

# BioMCP API Key - Enhanced trial data (works without key using public data)
BIOMCP_API_KEY=your_biomcp_key_here

# cBioPortal Token - Enhanced genomics data
CBIOPORTAL_TOKEN=your_cbioportal_token_here

# AlphaGenome API - Variant predictions
ALPHAGENOME_API_KEY=your_alphagenome_key_here

# ===========================
# OPTIONAL: Specialized Medical Models
# ===========================

# TrialGPT (NIH/NLM) - Research model for trial matching
TRIALGPT_API_URL=https://api.trialgpt.nlm.nih.gov/v1
TRIALGPT_API_KEY=your_trialgpt_key_here

# Meditron - Medical reasoning model (if you have access)
MEDITRON_API_KEY=your_meditron_key_here

# ===========================
# WORKING PERFORMANCE SETTINGS
# ===========================

# Maximum time budget per trial in milliseconds (default: 15000)
LLM_BUDGET_MS=15000

# Maximum cost budget per trial in USD (default: 0.05)
LLM_BUDGET_USD=0.05

# Cache TTL in seconds (default: 3600 = 1 hour)
CACHE_TTL_SECONDS=3600

# Maximum concurrent LLM calls (default: 5)
LLM_MAX_CONCURRENCY=5

# ===========================
# DEFINED BUT NOT FULLY IMPLEMENTED
# ===========================

# Default matching mode - currently doesn't affect model selection
# Options: fast, balanced, accurate (default: balanced)
DEFAULT_MODE=balanced

# Enable safety checks - safety checks always run regardless of this setting
ENABLE_SAFETY_CHECK=true

# ===========================
# LOGGING
# ===========================

# Log level: DEBUG, INFO, WARNING, ERROR (default: INFO)
LOG_LEVEL=INFO

# Optional log file path (logs to console if not set)
LOG_FILE=
